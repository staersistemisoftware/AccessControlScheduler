<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<window xmlns="http://schemas.haulmont.com/cuba/window.xsd"
        caption="msg://editorCaption"
        class="it.myefm.accesscontrolscheduler.web.tracciatobadgerlsostitutivi.TracciatoBadgeRLSostitutiviEdit"
        datasource="tracciatoBadgeRLSostitutiviDs"
        focusComponent="fieldGroup"
        messagesPack="it.myefm.accesscontrolscheduler.web.tracciatobadgerlsostitutivi">
    <dsContext>
        <datasource id="tracciatoBadgeRLSostitutiviDs"
                    class="it.myefm.accesscontrolscheduler.entity.TracciatoBadgeRLSostitutivi"
                    view="_local"/>
    </dsContext>
    <dialogMode height="600"
                width="800"/>
    <layout expand="windowActions"
            spacing="true">
        <fieldGroup id="fieldGroup"
                    datasource="tracciatoBadgeRLSostitutiviDs">
            <column width="250px">
                <field property="serialNumberBadgeRL"/>
                <field property="tipologiaTesseraBadgeRL"/>
                <field property="descrizioneTipologiaBadge"/>
                <field property="timeStamp"/>
            </column>
        </fieldGroup>
        <frame id="windowActions"
               screen="editWindowActions"/>
    </layout>
</window>
